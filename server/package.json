{
  "name": "amzserver",
  "version": "1.0.0",
  "description": "AWS Product API demo",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "dev": "nodemon ./server.js --exec babel-node -e js",
    "start": "babel-node ./server.js",
    "test": "jest --env=node --watch",
    "coverage": "jest --env=node --coverage"
  },
  "author": "Fadi Asfour",
  "license": "MIT",
  "devDependencies": {
    "apollo-server-express": "^1.3.4",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.2",
    "graphql-request": "^1.5.1",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "nodemon": "^1.17.3"
  },
  "dependencies": {
    "amazon-reviews-crawler": "^1.0.4",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "graphql-subscriptions": "^0.5.8",
    "graphql-tools": "^2.23.1",
    "mongoose": "^5.0.13",
    "nightmare": "^3.0.1",
    "random-ua": "^0.0.6",
    "request": "^2.85.0",
    "subscriptions-transport-ws": "^0.9.7"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "./dist"
    ],
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "moduleNameMapper": {
      "^mongoose$": "<rootDir>/node_modules/mongoose"
    }
  }
}
